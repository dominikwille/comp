#Packete einlesen:
import numpy as np
import csv
import matplotlib.pyplot as plt
from scipy import special

P = []
with open('P.txt', 'rb') as csvfile:
     File = csv.reader(csvfile, delimiter=' ')
     for row in File:
         if(row[0] != '#'):
             P.append(row)
     
print P[0]
